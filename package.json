{
  "name": "kekchat",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "c-build": "rollup -c",
    "c-dev": "rollup -c ./rollup.config.js -w",
    "c-start": "sirv client/public --no-clear",
    "c-validate": "svelte-check --tsconfig ./client/tsconfig.json",

    "s-build": "cargo build --release --manifest-path server/Cargo.toml",
    "s-run": "cargo run --release --manifest-path server/Cargo.toml"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "rollup": "^2.67.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.3",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "sirv-cli": "^2.0.2",
    "unidecode": "^0.1.8",
    "yarn": "^1.22.18"
  }
}
